rows.append(render_template('view_flight_formatting.html', customer_name = session_key, flight_count = count + 1, arrival_airport = data[count]['arrival_airport'], arrival_time = data[count]['arrival_time'], departure_airport = data[count]['departure_airport'],  departure_time = data[count]['departure_time'], flight_num = data[count]['flight_num'], ticket = data[count]['ticket_id'] ))
